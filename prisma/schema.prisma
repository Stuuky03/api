generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model student {
  id       String   @id
  username String   @unique
  email    String   @unique
  password String
  stuukies Stuuke[]
}

model Stuuke {
  id        String   @id
  title     String
  content   String
  createdAt DateTime
  student   student  @relation(fields: [studentId], references: [id])
  studentId String
}
