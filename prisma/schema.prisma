generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model student {
  id String @id
  firstName String
  lastName String
  username String @unique
  email String @unique
  password String
  profileImgUrl String @unique
  description String
  stuukies Stuuke[]
  }

model Stuuke {
  id String @id
  title String
  content String
  createdAt DateTime
  student student @relation(fields: [studentId], references: [id])
  studentId String
}